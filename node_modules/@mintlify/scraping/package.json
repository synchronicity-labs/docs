{
  "name": "@mintlify/scraping",
  "version": "3.0.87",
  "description": "Scrape documentation frameworks to Mintlify docs",
  "engines": {
    "node": ">=18.0.0"
  },
  "author": "Mintlify, Inc.",
  "bugs": {
    "url": "https://github.com/mintlify/docs/issues"
  },
  "license": "Elastic-2.0",
  "keywords": [
    "mintlify",
    "mint",
    "scraping"
  ],
  "type": "module",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "main": "./bin/index.js",
  "types": "./bin/index.d.ts",
  "bin": {
    "mintlify-scrape": "bin/cli.js"
  },
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc --project tsconfig.build.json",
    "clean:build": "rimraf bin",
    "clean:all": "rimraf node_modules .eslintcache && yarn clean:build",
    "watch": "tsc --watch",
    "test": "vitest run",
    "type": "tsc --noEmit",
    "lint": "eslint . --cache",
    "format": "prettier . --write",
    "format:check": "prettier . --check"
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.1.0",
    "axios": "^1.2.2",
    "cheerio": "^0.22.0",
    "fs-extra": "^11.1.1",
    "node-html-markdown": "^1.3.0",
    "ora": "^6.1.2",
    "puppeteer": "^19.4.0",
    "yargs": "^17.6.0"
  },
  "devDependencies": {
    "@mintlify/eslint-config": "1.0.5",
    "@mintlify/eslint-config-typescript": "1.0.10",
    "@mintlify/models": "0.0.69",
    "@mintlify/prettier-config": "1.0.4",
    "@mintlify/ts-config": "2.0.2",
    "@mintlify/validation": "0.1.126",
    "@trivago/prettier-plugin-sort-imports": "^4.2.1",
    "@tsconfig/recommended": "1.x",
    "@types/cheerio": "^0.22.31",
    "@types/node": "^18.7.13",
    "@types/yargs": "^17.0.13",
    "@typescript-eslint/eslint-plugin": "6.x",
    "@typescript-eslint/parser": "6.x",
    "eslint": "8.x",
    "eslint-config-prettier": "9.x",
    "eslint-plugin-unused-imports": "3.x",
    "openapi-types": "^12.1.3",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.1",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  },
  "gitHead": "24dc57a82346b6cec3e4a9359433f7555953820d"
}
